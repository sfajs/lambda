{
  "name": "@sfajs/lambda",
  "version": "1.3.1",
  "description": "将 sfa 托管到 serverless 云函数，如腾讯云云函数、阿里云云函数、awa lambda、azure functions 等",
  "homepage": "https://github.com/sfajs/lambda",
  "repository": {
    "type": "git",
    "url": "https://github.com/sfajs/lambda"
  },
  "keywords": [
    "sfa",
    "sfajs",
    "lambda",
    "serverless",
    "cloudbase",
    "azure functions",
    "awa lambda",
    "function compute"
  ],
  "bugs": {
    "url": "https://github.com/sfajs/lambda/issues",
    "email": "support@hal.wang"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "sh scripts/build.sh",
    "publish:beta": "sh scripts/build.sh && npm publish --tag=beta --access=public",
    "test": "npm run build && jest",
    "tests": "sh scripts/tests.sh",
    "lint": "eslint src test --ext .ts"
  },
  "author": "hal-wang",
  "license": "MIT",
  "dependencies": {
    "@sfajs/core": "^1.5.3"
  },
  "devDependencies": {
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.3",
    "@types/shelljs": "^0.8.11",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "codecov": "^3.8.3",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.2",
    "prettier": "^2.7.1",
    "shelljs": "^0.8.5",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "@sfajs/core": "~1"
  },
  "peerDependenciesMeta": {
    "@sfajs/core": {
      "optional": true
    }
  },
  "files": [
    "/.github/*",
    "/dist/*",
    "/scripts/*",
    "/src/*",
    "/test/*",
    ".eslintignore",
    ".eslintrc.js",
    ".gitignore",
    "CODE_OF_CONDUCT.md",
    "CONTRIBUTING.md",
    "jest.config.js",
    "LICENSE",
    "package.json",
    "README.md",
    "tsconfig.json"
  ]
